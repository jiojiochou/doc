 else if (prevSticky && !nextSticky) {\n      return -1\n    } else if (!prevSticky && nextSticky) {\n      return 1\n    }\n    return compareDate(prev, next)\n  })\n}\n\nexport function sortPostsByDate (posts) {\n  posts.sort((prev, next) => {\n    return compareDate(prev, next)\n  })\n}\n"],"mappings":";;;;;AAAA,SAASA,WAAT,QAA4B,sBAA5B,C,CAEA;;AACA,OAAO,SAASC,WAAT,CAAsBC,KAAtB,EAA6BC,UAA7B,EAAyC;EAC9CD,KAAK,GAAGA,KAAK,CAACE,MAAN,CAAa,UAACC,IAAD,EAAOC,KAAP,EAAiB;IACpC,IAAQC,KAAR,GAAwDF,IAAxD,CAAQE,KAAR;IAAA,wBAAwDF,IAAxD,CAAeG,WAAf;IAAA,IAA8BC,IAA9B,qBAA8BA,IAA9B;IAAA,IAAoCC,IAApC,qBAAoCA,IAApC;IAAA,IAA0CC,OAA1C,qBAA0CA,OAA1C,CADoC,CAEpC;;IACA,IAAIT,KAAK,CAACU,OAAN,CAAcP,IAAd,MAAwBC,KAA5B,EAAmC;MACjC,OAAO,KAAP;IACD,CAFD,MAEO;MACL,IAAMO,cAAc,GAAGJ,IAAI,KAAK,IAAT,IAAiBF,KAAK,IAAIO,SAA1B,IAAuCH,OAAO,KAAK,KAA1E;MACA,IAAMI,GAAG,GAAGZ,UAAU,KAAK,IAAf,GACR,EAAEU,cAAc,IAAIH,IAAI,KAAKI,SAA7B,CADQ,GAER,CAACD,cAFL;MAGA,OAAOE,GAAP;IACD;EACF,CAZO,CAAR;EAaA,OAAOb,KAAP;AACD,C,CAED;;AACA,OAAO,SAASc,wBAAT,CAAmCd,KAAnC,EAA0C;EAC/CA,KAAK,CAACe,IAAN,CAAW,UAACC,IAAD,EAAOC,IAAP,EAAgB;IACzB,IAAMC,UAAU,GAAGF,IAAI,CAACV,WAAL,CAAiBa,MAApC;IACA,IAAMC,UAAU,GAAGH,IAAI,CAACX,WAAL,CAAiBa,MAApC;;IACA,IAAID,UAAU,IAAIE,UAAlB,EAA8B;MAC5B,OAAOF,UAAU,IAAIE,UAAd,GAA2BtB,WAAW,CAACkB,IAAD,EAAOC,IAAP,CAAtC,GAAsDC,UAAU,GAAGE,UAA1E;IACD,CAFD,MAEO,IAAIF,UAAU,IAAI,CAACE,UAAnB,EAA+B;MACpC,OAAO,CAAC,CAAR;IACD,CAFM,MAEA,IAAI,CAACF,UAAD,IAAeE,UAAnB,EAA+B;MACpC,OAAO,CAAP;IACD;;IACD,OAAOtB,WAAW,CAACkB,IAAD,EAAOC,IAAP,CAAlB;EACD,CAXD;AAYD;AAED,OAAO,SAASI,eAAT,CAA0BrB,KAA1B,EAAiC;EACtCA,KAAK,CAACe,IAAN,CAAW,UAACC,IAAD,EAAOC,IAAP,EAAgB;IACzB,OAAOnB,WAAW,CAACkB,IAAD,EAAOC,IAAP,CAAlB;EACD,CAFD;AAGD"}]}